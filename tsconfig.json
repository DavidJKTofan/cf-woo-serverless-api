{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "es2022",
    "target": "es2022",
    "lib": ["esnext", "esnext.disposable", "dom"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "@types/jest",
      "@types/service-worker-mock"
    ]
  },
  "include": ["src", "worker-configuration.d.ts"],
  "exclude": ["node_modules", "dist", "test"]
}
